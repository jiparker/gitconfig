[color]
	ui = true
[user]
	email = cory.huebner@asolutions.com
	name = cory.huebner
[credential]
	helper = wincred

[difftool "codecompare"]
  cmd = 'C:\\Program Files\\Devart\\Code Compare\\codecompare.exe' "$LOCAL" "$REMOTE"
  renames = true

[difftool "tortoise"]
  cmd = \""c:/Program Files/TortoiseGIT/bin/TortoiseGitMerge.exe"\" -mine:"$LOCAL" -base:"$REMOTE"
 
[diff]
tool = codecompare
 
[mergetool "codecompare"]
cmd = 'C:\\Program Files\\Devart\\Code Compare\\codemerge.exe' -MF="$LOCAL" -TF="$REMOTE" -BF="$BASE" -RF="$MERGED"
trustExitCode = true

[mergetool "tortoise"]
    cmd = \"C:/Program Files/TortoiseSVN/bin/TortoiseMerge.exe\" -base:\"$BASE\" -theirs:\"$REMOTE\" -mine:\"$LOCAL\" -merged:\"$MERGED\"
 
[mergetool]
keepBackup = false
 
[merge]
tool = codecompare

[difftool]
prompt = false

[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	put = push origin HEAD
	rhu = !git reset --hard $(git rev-parse --symbolic-full-name @{u})
	dt = difftool
	pushu = push -u
	putu = push -u origin HEAD
	putqa = push origin HEAD:qa.internal
	deletebr = !sh -c 'git push origin :$1 && git branch -D $1' -